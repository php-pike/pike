{
    "name": "php-pike/pike",
    "version": "1.4.0",
    "description": "Pike is a extension library for ZendFramework1/Doctrine2 projects. Pike has out-of-the-box support for building complete DataGrids (jqGrid), Default variable escaping, reflection for resources, nice toolbar, EntityAudit, Paginator, Sessions with Doctrine, and many more cool stuff. Just check it out.",
    "keywords": ["datagrid", "zendframework", "jquery", "doctrine2", "grid", "EntityAudit", "Forms", "Toolbar", "Reflection"],
    "type": "library",
    "license": "MIT",
    "authors": [
    {
        "name": "Kees Schepers",
        "email": "kees@keesschepers.nl",
        "homepage": "http://keesschepers.nl/"
    },
    {
        "name": "Pieter Vogelaar",
        "email": "pieter@pietervogelaar.nl",
        "homepage": "http://pietervogelaar.nl/"
    }
    ],
    
    "require": {
        "php": ">=5.3",
        "doctrine/orm": "2.*",
	"zendframework/zendframework1" : "1.11.*"
    },
    
    "autoload":     {
        "psr-0": {
            "doctrine/doctrine2" : "lib/",
            "zendframework/zendframework1" : "library/",
            "Pike_" : "lib/"
        }
    },
    
    "include_path" : ["lib/"]
}
